
  <%= form_for @event do |f| %>
    <%= f.label :event_details %>
    <%= f.text_field :event_details %>
    <br>
    <%= f.label :event_price %>
    <%= f.text_field :event_price %>
    <br>
    <%= f.label :act_id, "Act" %>
    <%= f.collection_select(:act_id, @acts, :id, :act_name, prompt: "Select Act") %>
    <br>
    <%= f.label :act_id, "Act" %>
    <%= f.collection_select(:venue_id, @venues, :id, :venue_name, prompt: "Select Venue") %>
    <br>
    <%= f.label :event_start_time, "Start Time" %>
    <%= f.datetime_field :event_start_time%>
    <br>
    <%= f.label :event_end_time, "End Time" %>
    <%= f.datetime_field :event_end_time%>
    <br>
    <br>
    <%= f.submit 'Add Event!' %>
  <% end %>